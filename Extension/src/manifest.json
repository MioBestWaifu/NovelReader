{
    "manifest_version": 3,
    "name": "Maria-chan",
    "version": "0.4",
    "description": "Helper maid",
    "icons": {
      "32": "icons/maria32.png"
    },
    "permissions": ["activeTab","tabs","storage","webRequest","declarativeNetRequest"],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["text_tracker.js"]
      }
    ],
    "action":{
      "default_title":"Opens Maria's pages"
    }
}
  